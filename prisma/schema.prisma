generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model cliente {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  email    String
  nome     String
  telefone String
}

model funcionario {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  cargo     String
  cpf       String
  data_nasc DateTime @db.Date
  idade     Float
  nome      String
  root      Boolean
  senha     String
}

model vaga {
  id             String @id @default(auto()) @map("_id") @db.ObjectId
  /// Could not determine type: the field only had null or empty values in the sample set.
  clienteId      Json?
  dataLocacao    String
  /// Could not determine type: the field only had null or empty values in the sample set.
  dataLocacaoFim Json?
  preco          Float
  setor          String
  tipo           String
  vaga_ocupada   String
}

model veiculo {
  id        String @id @default(auto()) @map("_id") @db.ObjectId
  categoria String
  cor       String
  modelo    String
}
